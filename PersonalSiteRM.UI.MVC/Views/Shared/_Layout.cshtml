    <!---FOR EMAIL FORM-->
    <script>
    $("#ContactCreateForm").submit(function (e) {
        var formData = $(this).serializeArray();
        e.preventDefault();
        $.ajax({
            url: '@Url.Action("ContactAjax", "Home")',
            type: 'POST',
            data: formData,
            datatype: 'json',
            success: function (data) {
                $("#contactForm").hide();
                $("#contactConfirm").show();
                $("#contactConfirm").html('<p>' +
                    data.Name + ', your message has been received: <br/>' +
                    data.Message +
                    '<br/>', 'We will respond to' +
                    data.Email +
                    'as soon as we can. Thank you for your email!'
                );
            }
        });
        });

    </script>
    <!---FOR SCROLL FUNCTION IN JQUERY-->
    <script>
        $(document).ready(function () {
        $('#nav').onePageNav();
        });
    </script>

